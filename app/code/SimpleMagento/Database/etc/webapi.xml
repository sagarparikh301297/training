<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">

    <route method="GET" url="/V1/affiliate_member">
        <service class="SimpleMagento\Database\Api\AffiliateMemberRepositoryInterface" method="getList" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>

    <route method="GET" url="/V1/affiliate_member/:id">
        <service class="SimpleMagento\Database\Api\AffiliateMemberRepositoryInterface" method="getAffiliateMemberById" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>

    <route method="POST" url="/V1/affiliate_member">
        <service class="SimpleMagento\Database\Api\AffiliateMemberRepositoryInterface" method="saveAffiliateMember" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>

    <route method="PUT" url="/V1/affiliate_member/:id">
        <service class="SimpleMagento\Database\Api\AffiliateMemberRepositoryInterface" method="saveAffiliateMember" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>

    <route method="DELETE" url="/V1/affiliate_member/:id">
        <service class="SimpleMagento\Database\Api\AffiliateMemberRepositoryInterface" method="deleteById" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>

    <route method="GET" url="/V1/affiliate_member/search">
        <service class="SimpleMagento\Database\Api\AffiliateMemberRepositoryInterface" method="getSearchResultsList" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>

    </routes>